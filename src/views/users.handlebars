<div class="p-3">
    <h2 class="text-center m-2">Listado de usuarios</h5>
    {{#if hasUsers}}
    <table class="table">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Apellido</th>
      <th scope="col">Email</th>
      <th scope="col">Rol</th>
      <th scope="col">Acciones</th>
    </tr>
  </thead>
  <tbody>
       {{#each usersList}} 
            <tr>
                <td>{{this.first_name}}</td>
                <td>{{this.last_name}}</td>
                <td>{{this.email}}</td>
                <td>{{this.role}}</td>
                <td><div class="d-flex justify-content-end pe-2">
                    <span class="pe-2">Editar</span>
                    <span>Eliminar</span>
                </div></td>
            </tr>
       {{/each}}
  </tbody>
</table>
{{else}}
    <p class="text-center m-4">AÃºn no hay productos cargados en el carrito</p>
{{/if}}
</div>